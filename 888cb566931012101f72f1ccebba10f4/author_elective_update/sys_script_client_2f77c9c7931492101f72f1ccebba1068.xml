        <condition/>
        <description>Assigned Person field will be based off of the Group Assigned field.</description>
        <field>group_assigned</field>
        <global>true</global>
        <isolate_script>false</isolate_script>
        <messages/>
        <name>Person Select</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }

    var ga = new GlideAjax('GetUsersByGroup');
    ga.addParam('sysparm_name', 'getUsers');
    ga.addParam('groupId', newValue);
    ga.getXMLAnswer(function(response) {
        var userIds = response.responseXML.documentElement.getAttribute("answer");
        g_form.setValue('assigned_person', '');
        g_form.getField('assigned_person').reference_qual = 'sys_idIN' + userIds;
        g_form.refreshOptions('assigned_person');
    });
}]]></script>
